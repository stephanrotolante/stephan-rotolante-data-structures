{
  "version": "independent",
  "npmClient": "npm",
  "command": {
    "publish": {
      "ignoreChanges": ["ignored-file", "*.md", "**/package-lock.json"],
      "message": "chore(release): publish",
      "registry": "https://npm.pkg.github.com",
      "conventionalCommits": true,
      "verifyAccess": false
    },
    "diff": {
      "ignoreChanges": ["**/*.md", "**/package-lock.json"]
    }
  },
  "ignoreChanges": ["**/*.md", "**/package-lock.json"],
  "packages": ["structures/*"]
}
